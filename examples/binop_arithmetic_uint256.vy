# @version ^0.3.4

@external
@view
def add_uint256(a: uint256, b: uint256) -> uint256:
    return a + b

@external
@view
def sub_uint256(a: uint256, b: uint256) -> uint256:
    return a - b

@external
@view
def mul_uint256(a: uint256, b: uint256) -> uint256:
    return a * b

@external
@view
def div_uint256(a: uint256, b: uint256) -> uint256:
    return a / b
